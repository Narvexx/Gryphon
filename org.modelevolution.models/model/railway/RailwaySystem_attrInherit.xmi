<?xml version="1.0" encoding="ASCII"?>
<rw:RailwaySystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rw="http://rw.modelevolution.org/1.0"
    xsi:schemaLocation="http://rw.modelevolution.org/1.0 railwaysystem_attrInherit.ecore">
  <trains route="//@routes.0"/>
  <tracks xsi:type="rw:Entry"
      id="ENTRY"
      next="//@tracks.1"
      test="-1"/>
  <tracks xsi:type="rw:Track"
      id="AA"
      next="//@tracks.2"
      test="1"/>
  <tracks xsi:type="rw:Exit"
      id="EXIT"
      test="-2"/>
  <routes tracks="//@tracks.0 //@tracks.1 //@tracks.2"
      id="R10A"/>
</rw:RailwaySystem>
